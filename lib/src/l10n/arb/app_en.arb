{
  "@@locale": "en",
  "appTitle": "Flutter Puzzle Hack",
  "@appTitle": {
    "description": "The title of the application"
  },
  "puzzlePageTitle": "Flutter Puzzle Hack",
  "@puzzlePageTitle": {
    "description": "The title of Puzzle Page"
  },
  "puzzlePageSubTitle": "To win, wash the duck.",
    "@appTitle": {
      "description": "The subtitle of the application"
  },
  "buttonStartText": "Start Game",
  "@buttonStartText": {
    "description": "Start the game"
  },
  "buttonRestartText": "Restart Game",
  "@buttonRestartText": {
    "description": "Restart the game"
  },
  "puzzleDurationLabelText": "{hours} hours {minutes} minutes {seconds} seconds",
  "@puzzleDurationLabelText": {
      "description": "Semantic for the puzzle timer",
      "placeholders": {
          "hours": {
              "type": "String",
              "example": "1"
          },
          "minutes": {
              "type": "String",
              "example": "21"
          },
          "seconds": {
              "type": "String",
              "example": "42"
          }
      }
  },
  "nMoves": "{count,plural, =0{{count} move} =1{{count} move} other{{count} moves}}",
  "@nMoves": {
    "description": "Shown as a number of moves",
    "placeholders": {
      "count": {
        "type": "int"
      }
    }
  },
  "puzzleVictoryDialogTitle": "You've Won!",
  "@puzzleVictoryDialogTitle": {
    "description": "Title of the victory dialog"
  },
  "puzzleVictoryDialogDescription": "Your score:",
  "@puzzleVictoryDialogDescription": {
    "description": "Description of the victory dialog"
  },
  "buttonCloseLabel": "Close",
  "@buttonCloseLabel": {
    "description": "Close button label of the victory dialog"
  }
}