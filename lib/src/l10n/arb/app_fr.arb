{
  "@@locale": "fr",
  "appTitle": "Flutter Puzzle Hack",
  "@appTitle": {
    "description": "Le titre de l'application"
  },
  "puzzlePageTitle": "Flutter Puzzle Hack",
  "@puzzlePageTitle": {
    "description": "Le titre de la page du puzzle"
  },
  "buttonStartText": "Commencer",
  "@buttonStartText": {
    "description": "Démarrage du jeu"
  },
  "buttonRestartText": "Recommencer",
  "@buttonRestartText": {
    "description": "Redémarrage du jeu"
  },
  "puzzleDurationLabelText": "{hours} heures {minutes} minutes {seconds} secondes",
  "@puzzleDurationLabelText": {
      "description": "Sémantique pour le chronomètre du puzzle",
      "placeholders": {
          "hours": {
              "type": "String",
              "example": "1"
          },
          "minutes": {
              "type": "String",
              "example": "21"
          },
          "seconds": {
              "type": "String",
              "example": "42"
          }
      }
  },
  "nMoves": "{count,plural, =0{{count} mouvement} =1{{count} mouvement} other{{count} mouvements}}",
  "@nMoves": {
    "description": "Affiché en tant que nombre de mouvements",
    "placeholders": {
      "count": {
        "type": "int"
      }
    }
  }
}